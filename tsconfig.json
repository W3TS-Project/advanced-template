{
    "compilerOptions": {
        "target": "es6",
        "removeComments": true,
        "lib": ["es6"],
        "strict": false,
        "types": [
            "lua-types/core/coroutine",
            "lua-types/core/global",
            "lua-types/core/math",
            "lua-types/core/metatable",
            "lua-types/core/modules",
            "lua-types/core/string",
            "lua-types/core/table",
            "lua-types/core/os",
            "lua-types/special/5.3",
            "lua-types/5.3",
            "ceres-decl/ceres",
            "war3-types/core/compat",
            "war3-types/core/common",
            "war3-types/core/blizzard",
            "war3-types/core/commonai",
            "war3-types/core/polyfill",
            "war3-types/special/w3ts",
            "./src/war3map",
            "./src/decl"
        ],
        "allowJs": true,
        "alwaysStrict": true,
        "checkJs": false,
        "moduleResolution": "classic",
        "outDir": "target/compiled",
        "baseUrl": "./node_modules",
        "paths": {
            "*": ["./*/src/index", "./*/lib/index", "./*/index", "./*"]
        },
        "declaration": false
    },
    "include": [
        "node_modules/ceres-ts-utils",
        "node_modules/obj-edit",
        "node_modules/ident-wrap-ts",
        "src",
        "main.ts"
    ],
    "tstl": {
        "luaTarget": "5.3",
        "noHeader": true,
        "luaLibImport": "require",
        "noImplicitSelf": true,
    }
}
